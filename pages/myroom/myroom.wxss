/* pages/myroom/myroom.wxss */
.true{
  background-color: white;
  font-size: 0.7em;
  font-weight: 700;
  height: 220rpx;
  margin:5rpx;
}
.description{
  width:100%;
  height:100rpx;
  border-style: double double double double;
  border-radius:5px;
  font-weight: 500;
  font-size: 0.9em;
  text-align: left;
}
.description text{
  display: -webkit-box;
  font-size:0.9em;
  line-height: 30rpx;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:3;
  overflow: hidden;
  text-overflow:ellipsis;
}
.numbers{
  text-align: left;
  color: gainsboro;
  font-size: 0.7em;
}
.title{
  text-align: center;
  width: 100%;
}