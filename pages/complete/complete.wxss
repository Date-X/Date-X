/* pages/complete/complete.wxss */
page{
  background-color: #EAEAEA;
}
.title {
  width: 250px;
  text-align: center;
  font-size: 30px;
  margin: 0 auto;
  margin-bottom: 30rpx;
  border-bottom: solid 1rpx;
}

.sex-container {
  height: 100px;
}

.gender-img{
  height: 35rpx;
  width: 35rpx;
  margin-left: 2rpx;
}

.pre-container {
  display: flex;
  flex-direction: column;
}

.sex,.pre {
  margin-left: 60px
}

.radio-group  {
  width: 60%;
  margin: 0 auto;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.radio {
  width: 180rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}


.checkbox-group{
  width: 80%;
  margin: 0 auto;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-around;
}

.section {
  height: 250rpx;
  width: 250rpx;
  flex: auto;
  display: flex;
  flex-direction: column;
  margin-top: 10rpx;
}

.img{
  height: 200rpx;
  width: 200rpx;
  border-radius: 50%;
}

button {
  width:250rpx;
  margin-top:35rpx;
}